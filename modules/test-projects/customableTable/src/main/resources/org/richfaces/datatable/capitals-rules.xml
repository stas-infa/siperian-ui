<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	<!DOCTYPE digester-rules SYSTEM "digester-rules.dtd">
-->

<digester-rules>
	<pattern value="*/capital">
		<object-create-rule	classname="org.richfaces.datatable.Capital" />
		<bean-property-setter-rule pattern="name" propertyname="name"/>	
		<bean-property-setter-rule pattern="state" propertyname="state"/>	
		<bean-property-setter-rule pattern="timeZone" propertyname="timeZone"/>	
		<bean-property-setter-rule pattern="population" propertyname="population"/>	
		<bean-property-setter-rule pattern="buget" propertyname="buget"/>	
		<bean-property-setter-rule pattern="description" propertyname="description"/>	
		<set-next-rule methodname="addCapital"/>
	</pattern>
</digester-rules>