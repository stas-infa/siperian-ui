<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
  version="1.2">	
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
	<converter>
		<display-name>columnsConverter</display-name>
		<converter-id>columnsConverter</converter-id>
		<converter-class>org.richfaces.datatable.ColumnsConverter</converter-class>
	</converter>
	<!-- queue.xhtml -->
	<managed-bean>
		<managed-bean-name>skinBean</managed-bean-name>
		<managed-bean-class>org.richfaces.SkinBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		filteringBean</managed-bean-name>
		<managed-bean-class>
		org.richfaces.datatable.FilteringBean</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pageBean</managed-bean-name>
		<managed-bean-class>org.richfaces.datatable.PageBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>capitalsBean</managed-bean-name>
		<managed-bean-class>org.richfaces.datatable.CapitalsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataModelBean</managed-bean-name>
		<managed-bean-class>org.richfaces.sample.DataModelBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataActionBean</managed-bean-name>
		<managed-bean-class>org.richfaces.sample.DataActionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>